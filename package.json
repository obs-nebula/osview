{
  "name": "osview",
  "version": "1.0.0",
  "description": "Basic example generating traces and metrics from a CLI with OpenTelemetry-JS",
  "main": "./dist/index.js",
  "bin": {
    "osview": "./dist/index.js"
  },
  "scripts": {
    "prestart": "npm run lint",
    "start": "node .",
    "lint": "eslint . --ext .ts",
    "compile": "tsc --build tsconfig.json",
    "clean": "rm -Rf dist"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.7",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.4.1",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.38.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.38.0",
    "@opentelemetry/resources": "^1.12.0",
    "@opentelemetry/sdk-metrics": "1.11.0",
    "@opentelemetry/sdk-trace-base": "^1.12.0",
    "@opentelemetry/semantic-conventions": "^1.12.0"
  }
}
